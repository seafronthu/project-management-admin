$color(cor = false)
  if cor
    color cor
  else
    color rgba(0, 0, 0, .65)
$bgColor(cor = false)
  if cor
    background-color cor
  else
    background-color rgba(255, 255, 255, .25)
$themeColor()
  $color($theme)
$themeBgColor()
  $bgColor($theme)
$activeBgColor()
  $bgColor(lighten($theme, 90%))
